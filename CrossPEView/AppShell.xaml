<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CrossPEView.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    
    xmlns:local="clr-namespace:CrossPEView.Page"
    Shell.FlyoutBehavior="Disabled"
    Title="CrossPEView">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Import"
        ContentTemplate="{DataTemplate local:ImportPage}"
        Route="ImportPage" />
    <ShellContent
        Title="PE分析工具"
        ContentTemplate="{DataTemplate local:DoMainPage}"
        Route="DoMainPage" />
</Shell>
